    <Compile Update="Secundario\Especifico\CursoUI.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Secundario\Especifico\PlanUI.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Secundario\General\OperacionExitosa.cs">
      <SubType>Form</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>