    <Compile Update="Secundario\Especifico\DictadoUI.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Secundario\Especifico\UsuarioUI.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Secundario\Especifico\MateriaUI.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Secundario\General\ErrorBaseDeDatos.cs">
      <SubType>Form</SubType>
