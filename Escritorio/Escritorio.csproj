      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Secundario\Especifico\InscripcionAlumnoUI.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Secundario\Especifico\InscripcionCursoDocenteUI.cs">
